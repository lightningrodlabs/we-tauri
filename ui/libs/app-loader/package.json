{
  "name": "@neighbourhoods/app-loader",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "./*": {
      "types": "./dist/",
      "import": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "prepare": "pnpm build",
    "build": "vite build && tsc --build",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest --reporter=verbose"
  },
  "devDependencies": {
    "@neighbourhoods/design-system-components": "workspace:*",
    "@neighbourhoods/design-system-styles": "workspace: *",
    "@open-wc/testing": "^3.2.2",
    "@scoped-elements/shoelace": "^0.1.0",
    "@webcomponents/scoped-custom-element-registry": "^0.0.9",
    "jsdom": "^22.1.0",
    "lit": "^3.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.4"
  },
  "dependencies": {
    "@lit-labs/scoped-registry-mixin": "^1.0.1",
    "@holochain-open-dev/utils": "^0.16.3",
    "@holochain/client": "^0.16.7",
    "@holochain-open-dev/profiles": "^0.17.3",
    "@neighbourhoods/client": "workspace:*"
  }
}
