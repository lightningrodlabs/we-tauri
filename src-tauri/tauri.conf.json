{
  "build": {
    "devPath": "http://localhost:1420",
    "distDir": "../ui/app/dist",
    "withGlobalTauri": false,
    "beforeBuildCommand": "npm run build:ui"
  },
  "package": {
    "productName": "We",
    "version": "0.0.13"
  },
  "tauri": {
    "cli": {
      "description": "Running we from the command line",
      "args": [
        {
          "name": "profile",
          "takesValue": true,
          "multiple": false
        },
        {
          "name": "network-seed",
          "takesValue": true,
          "multiple": false
        }
      ]
    },
    "allowlist": {
      "protocol": {
        "all": true
      },
      "clipboard": {
        "writeText": true
      },
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "http": {
        "all": true,
        "request": true,
        "scope": ["applet://*"]
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "externalBin": ["bins/lair-keystore-v0.2.3"],
      "identifier": "we",
      "targets": "all"
    },
    "security": {
      "csp": "default-src 'self'; frame-src applet: https://applet.localhost/",
      "dangerousDisableAssetCspModification": true
    },
    "updater": {
      "active": false
    },
    "windows": []
  }
}
