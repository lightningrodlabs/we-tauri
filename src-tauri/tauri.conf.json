{
  "build": {
    "devPath": "../ui/app/dist",
    "distDir": "../ui/app/dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "we",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true,
        "scope": ["$APPDATA/webhapps/*"]
      },
      "http": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "externalBin": ["bins/lair-keystore-v0.2.3"],
      "identifier": "we",
      "targets": "all"
    },
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true
    },
    "updater": {
      "active": false
    },
    "windows": []
  }
}
