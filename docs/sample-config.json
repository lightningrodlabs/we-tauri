{
    "neighbourhood": "Rated Agenda",
    "wizard-version": "v0.1",
    "config-version": "v1-inclusive",
    "creator": "John Doe <john@doe.org>",
    "requiredApps": [ { "name":"AgendaItem", "version":"v1.0" } ],
    "ranges": [
        { "name": "timeRange", "kind": "Integer", "min": "1", "max": "600" },
        { "name": "voteRange", "kind": "Integer", "min": 0, "max": 1 },
        { "name": "ratioRange", "kind": "Float", "min": "0.0", "max": "1.0" }
    ],
    "dimensions": [
        { "name": "duration", "range": "timeRange" },
        { "name": "upvote", "range": "voteRange" },
        { "name": "upvoteRatio", "range": "ratioRange", "computed": true }
    ],
    "resources": [
        {
            "name": "AgendaItemResource",
            "base_type": "AgendaItem",
            "dimensions": [ { "name": "duration", "range": "timeRange" }, { "name": "upvote", "range": "voteRange" }, { "name": "upvoteRatio", "range": "ratioRange", "computed": true } ]
        }
    ],
    "methods": [
        {
            "name": "computeUpvoteRatio",
            "target_resource_type": "AgendaItemResource",
            "input_dimensions": [ { "name": "upvote", "range": "voteRange" } ],
            "output_dimension": { "name": "upvoteRatio", "range": "ratioRange", "computed": true },
            "program": "Sum",
            "can_compute_live": false, // will do live method computation each time a new assessment comes in
            "must_publish_dataset": false // publish the dataset used for each computation
        }
        
    ],
    "contexts": [
        {
            "name": "PrioritizedAgenda",
            "resource_type": "AgendaItemResource",
            "thresholds": [
                { "dimension": { "name": "duration", "range": "timeRange" }, "kind": "LessThan", "value": { "Integer": "2"} },
                { "dimension": { "name": "upvoteRatio", "range": "ratioRange", "computed": true }, "kind": "GreaterThan", "value": { "Float": "0.0" } }
            ],
            "order_by": [
                { "dimension": { "name": "upvoteRatio", "range": "ratioRange", "computed": true }, "order": "Biggest" },
                { "dimension": { "name": "duration", "range": "timeRange" }, "order": "Biggest" }
            ]
        }
    ]
}