{
  "name": "@neighbourhoods/component-storybook",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "npm run storybook",
    "build": "npm run build:design-system",
    "build:design-system": "cd design-system && npm run build",
    "build:dev-utils": "cd dev-util && npm run build",
    "clean": "npm run clean:build && npm run clean:modules",
    "clean:build": "rimraf storybook-static; rimraf design-system/dist; rimraf dev-util/dist; exit 0",
    "clean:modules": "rimraf node_modules; for DIR in $(find -type d -iname node_modules); do echo \"  Remove $DIR\"; rm -Rf \"$DIR\"; done",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@base2/pretty-print-object": "^1.0.2",
    "@mdx-js/react": "^2.3.0",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/blocks": "^7.3.2",
    "@storybook/builder-vite": "^7.3.2",
    "@storybook/web-components": "^7.3.2",
    "@storybook/web-components-vite": "^7.3.2",
    "babel-loader": "^8.2.3",
    "react": "^17.0.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "storybook": "^7.3.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@lit-labs/scoped-registry-mixin": "^1.0.1",
    "@neighbourhoods/design-system-styles": "^0.0.3",
    "@scoped-elements/shoelace": "^0.1.0",
    "lit": "^2.2.0"
  }
}
